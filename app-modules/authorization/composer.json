{
  "name": "canyon-gbs/authorization",
  "description": "",
  "type": "library",
  "version": "1.0",
  "license": "proprietary",
  "require": {
    "filament/filament": "^3.0-stable",
    "spatie/laravel-permission": "^5.10"
  },
  "autoload": {
    "psr-4": {
      "Assist\\Authorization\\": "src/",
