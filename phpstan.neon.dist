includes:
    - phpstan-baseline.neon
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app/
        - app-modules/
        - database/
        - tests/
        - routes/

    scanFiles:
          - _ide_helper_models.php

    excludePaths:
          - app-modules/*/vendor/*

    # Level 9 is the highest level
    level: 1
